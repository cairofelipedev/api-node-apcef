{
  "kind": "collectionType",
  "collectionName": "noticias",
  "info": {
    "name": "Not√≠cias"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": {
      "type": "string"
    },
    "subtitulo": {
      "type": "string"
    },
    "descricao": {
      "type": "richtext"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "date": {
      "type": "datetime"
    },
    "slug": {
      "type": "uid",
      "targetField": "titulo"
    },
    "tipo": {
      "type": "enumeration",
      "enum": [
        "apcefpi",
        "fenae"
      ]
    }
  }
}
